[{"id":6091,"surah":94,"no_ayah":1,"tema":"","text_ayah":"\u0623\u064e\u0644\u064e\u0645\u0652 \u0646\u064e\u0634\u0652\u0631\u064e\u062d\u0652 \u0644\u064e\u0643\u064e \u0635\u064e[q:14548[\u062f\u0652]\u0631\u064e\u0643\u064e","text_id":"Bukankah Kami telah melapangkan dadamu (Nabi Muhammad),","text_en":"HAVE WE NOT opened up your breast","transliteration":"Alam nashrah laka sadrak","no_fn":"","text_fn":""},{"id":6092,"surah":94,"no_ayah":2,"tema":"","text_ayah":"\u0648\u064e\u0648\u064e\u0636\u064e\u0639\u0652\u0646\u064e\u0627 \u0639\u064e[f:626[\u0646\u0643]\u064e \u0648\u0650\u0632\u0652\u0631\u064e\u0643\u064e","text_id":"meringankan beban (tugas-tugas kenabian) darimu","text_en":"And removed your burden","transliteration":"Wa wa d'ana 'anka wizrak","no_fn":"","text_fn":""},{"id":6093,"surah":94,"no_ayah":3,"tema":"","text_ayah":"\u0671\u0644\u0651\u064e\u0630[o[\u0650\u064A\u0653] \u0623\u064e[f:14549[\u0646\u0642]\u064e\u0636\u064e \u0638\u064e\u0647\u0652\u0631\u064e\u0643\u064e","text_id":"yang memberatkan punggungmu,","text_en":"Which had left you devoid of hope,","transliteration":"Allazee anqada zahrak","no_fn":"","text_fn":""},{"id":6094,"surah":94,"no_ayah":4,"tema":"","text_ayah":"\u0648\u064e\u0631\u064e\u0641\u064e\u0639\u0652\u0646\u064e\u0627 \u0644\u064e\u0643\u064e \u0630\u0650\u0643\u0652\u0631\u064e\u0643\u064e","text_id":"dan meninggikan (derajat)-mu (dengan selalu) menyebut-nyebut (nama)-mu?","text_en":"And exalted your fame?","transliteration":"Wa raf 'ana laka zikrak","no_fn":"","text_fn":""},{"id":6095,"surah":94,"no_ayah":5,"tema":"","text_ayah":"\u0641\u064e\u0625\u0650[g[\u0646\u0651]\u064e \u0645\u064e\u0639\u064e [h:14550[\u0671]\u0644\u0652\u0639\u064f\u0633\u0652\u0631\u0650 \u064a\u064f\u0633\u0652\u0631\u064b\u0627","text_id":"Maka, sesungguhnya beserta kesulitan ada kemudahan.","text_en":"Surely with hardship there is ease.","transliteration":"Fa inna ma'al usri yusra","no_fn":"","text_fn":""},{"id":6096,"surah":94,"no_ayah":6,"tema":"","text_ayah":"\u0625\u0650[g[\u0646\u0651]\u064e \u0645\u064e\u0639\u064e [h:14550[\u0671]\u0644\u0652\u0639\u064f\u0633\u0652\u0631\u0650 \u064a\u064f\u0633\u0652\u0631\u064b\u0627","text_id":"Sesungguhnya beserta kesulitan ada kemudahan.","text_en":"With hardship indeed there is ease.","transliteration":"Inna ma'al 'usri yusra","no_fn":"","text_fn":""},{"id":6097,"surah":94,"no_ayah":7,"tema":"","text_ayah":"\u0641\u064e\u0625\u0650\u0630\u064e\u0627 \u0641\u064e\u0631\u064e\u063a\u0652\u062a\u064e \u0641\u064e[h:14551[\u0671][f:14551[\u0646\u0635]\u064e[q:14551[\u0628\u0652]","text_id":"Apabila engkau telah selesai (dengan suatu kebajikan), teruslah bekerja keras (untuk kebajikan yang lain)","text_en":"So when you are free work diligently,","transliteration":"Fa iza faragh ta fansab","no_fn":"","text_fn":""},{"id":6098,"surah":94,"no_ayah":8,"tema":"","text_ayah":"\u0648\u064e\u0625\u0650\u0644\u064e\u064A\u0670 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0641\u064e[h:14552[\u0671]\u0631\u0652\u063a\u064e[q[\u0628]","text_id":"dan hanya kepada Tuhanmu berharaplah!","text_en":"And turn to your Lord with all your love.","transliteration":"Wa ilaa rabbika far ghab","no_fn":"","text_fn":""}]