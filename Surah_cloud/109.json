[{"id":6208,"surah":109,"no_ayah":1,"tema":"","text_ayah":"\u0642\u064f\u0644\u0652 \u064a\u064e[o[\u0640\u0670\u0653]\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 [h:14663[\u0671]\u0644\u0652\u0643\u064e[n[\u0640\u0670]\u0641\u0650\u0631[p[\u064f\u0648]\u0646\u064e","text_id":"Katakanlah (Nabi Muhammad), “Wahai orang-orang kafir,","text_en":"SAY: \"O YOU unbelievers,","transliteration":"Qul yaa-ai yuhal kaafiroon","no_fn":"","text_fn":""},{"id":6209,"surah":109,"no_ayah":2,"tema":"","text_ayah":"\u0644[o[\u064e\u0627\u0653] \u0623\u064e\u0639\u0652\u0628\u064f\u062f\u064f \u0645\u064e\u0627 \u062a\u064e\u0639\u0652\u0628\u064f\u062f[p[\u064f\u0648]\u0646\u064e","text_id":"aku tidak akan menyembah apa yang kamu sembah.","text_en":"I do not worship what you worship,","transliteration":"Laa a'budu ma t'abudoon","no_fn":"","text_fn":""},{"id":6210,"surah":109,"no_ayah":3,"tema":"","text_ayah":"\u0648\u064e\u0644[o[\u064e\u0627\u0653] \u0623\u064e[f:428[\u0646\u062a]\u064f\u0645\u0652 \u0639\u064e[n[\u0640\u0670]\u0628\u0650\u062f\u064f\u0648\u0646\u064e \u0645[o[\u064e\u0627\u0653] \u0623\u064e\u0639\u0652\u0628\u064f[q[\u062f]\u064f","text_id":"Kamu juga bukan penyembah apa yang aku sembah.","text_en":"Nor do you worship who I worship,","transliteration":"Wa laa antum 'aabidoona maa a'bud","no_fn":"","text_fn":""},{"id":6211,"surah":109,"no_ayah":4,"tema":"","text_ayah":"\u0648\u064e\u0644[o[\u064e\u0627\u0653] \u0623\u064e\u0646\u064e\u0627\u06df \u0639\u064e\u0627\u0628\u0650[a:14664[\u062f\u064c \u0645]\u0651\u064e\u0627 \u0639\u064e\u0628\u064e[d:14665[\u062f]\u062a\u0651\u064f\u0645\u0652","text_id":"Aku juga tidak pernah menjadi penyembah apa yang kamu sembah.","text_en":"Nor will I worship what you worship,","transliteration":"Wa laa ana 'abidum maa 'abattum","no_fn":"","text_fn":""},{"id":6212,"surah":109,"no_ayah":5,"tema":"","text_ayah":"\u0648\u064e\u0644[o[\u064e\u0627\u0653] \u0623\u064e[f:428[\u0646\u062a]\u064f\u0645\u0652 \u0639\u064e[n[\u0640\u0670