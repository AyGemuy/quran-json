[{"id":6231,"surah":114,"no_ayah":1,"theme":"","text_id":"Katakanlah (Nabi Muhammad), “Aku berlindung kepada Tuhan manusia,","text_en":"SAY: \"I SEEK refuge with the Lord of men,","transliteration":"qul a'ụżu birabbin-nās","no_fn":"","text_fn":""},{"id":6232,"surah":114,"no_ayah":2,"theme":"","text_id":"raja manusia,","text_en":"The King of men,","transliteration":"malikin-nās","no_fn":"","text_fn":""},{"id":6233,"surah":114,"no_ayah":3,"theme":"","text_id":"sembahan manusia","text_en":"The God of men,","transliteration":"ilāhin-nās","no_fn":"","text_fn":""},{"id":6234,"surah":114,"no_ayah":4,"theme":"","text_id":"dari kejahatan (setan) pembisik yang bersembunyi","text_en":"From the evil of him who breathes temptations into the minds of men,","transliteration":"min syarril-waswāsil-khannās","no_fn":"","text_fn":""},{"id":6235,"surah":114,"no_ayah":5,"theme":"","text_id":"yang membisikkan (kejahatan) ke dalam dada manusia,","text_en":"Who suggests evil thoughts to the hearts of men --","transliteration":"allażī yuwaswisu fī ṣudụrin-nās","no_fn":"","text_fn":""},{"id":6236,"surah":114,"no_ayah":6,"theme":"","text_id":"dari (golongan) jin dan manusia.”","text_en":"From among the jinns and men.","transliteration":"minal-jinnati wan-nās","no_fn":"","text_fn":""}]